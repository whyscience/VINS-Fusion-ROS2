<?xml version="1.0" encoding="UTF-8"?>
<launch>

    <let name="rviz_config" value="$(find-pkg-share vins)/config/vins_rviz_config.rviz"/>
    <node name="rviz2" pkg="rviz2" exec="rviz2" args="-d $(var rviz_config)"/>
    
    <let name="vins_config" value="$(find-pkg-share vins)/config/euroc/euroc_mono_imu_config.yaml"  />
    <node name="vins_node" pkg="vins" exec="vins_node" args=" $(var vins_config)"/>
    
</launch>
